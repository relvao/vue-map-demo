{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "strictPropertyInitialization": false,
      "lib": [
        "esnext",
        "esnext.asynciterable",
        "dom"
      ],
      "esModuleInterop": true,
      "allowJs": true,
      "sourceMap": true,
      "strict": true,
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
        "~/*": [
          "./*"
        ],
        "@/*": [
          "./*"
        ]
      },
      "types": [
        "@types/node",
        "@nuxt/types",
        "node_modules/path-to-regexp/dist/index.d.ts",
        "nuxt-leaflet"
      ]
    },
    "exclude": [
      "node_modules"
    ]
  }